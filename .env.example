# To query the vector database and index docs 
UPSTASH_VECTOR_REST_URL=your_upstash_vector_rest_url
UPSTASH_VECTOR_REST_TOKEN=your_upstash_vector_rest_token

# While indexing docs to the vector database, prevents indexing documents that are too short
MIN_FILE_LENGTH=10 # default is 10

UPSTASH_VECTOR_INDEX_NAMESPACE=your_upstash_vector_index_namespace # default is 'docusaurus-search-upstash'

# (Optional) To generate embeddings for the indexed documents
# If not provided, embeddings will be generated by Upstash Vector
OPENAI_API_KEY=your_openai_api_key

# (Optional) To create a schedule to index the documents with the script provided
QSTASH_TOKEN=your_qstash_token
# The URL of the docs deployment
DOCS_DEPLOYMENT_URL=your_docs_deployment_url